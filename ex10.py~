tabby_cat = "\tI'm \btabbed in"
persian_cat = "I'm split\non a line."
backslash_cat = "I'm \\ a \\ cat."

fat_cat = '''
I'll do a list:
\t* Cat fo\aod
\t* Fishies
\t* Catnip\n\t* Grass
'''

print tabby_cat
print persian_cat
print backslash_cat
print fat_cat + "for cat's wh\ro say %s" % tabby_cat

while True:
    for i in ["/","-","|","\\","|"]:
        print "%s\r" % i,
